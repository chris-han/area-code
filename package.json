{
  "name": "area-code",
  "version": "0.3.0",
  "private": true,
  "workspaces": [
    "ufa/apps/*",
    "ufa/packages/*",
    "ufa/services/*",
    "odw/apps/*",
    "odw/packages/*",
    "odw/services/*"
  ],
  "scripts": {
    "ufa:dev": "NODE_ENV=development turbo run ufa:dev --ui=tui",
    "ufa:dev:clean": "turbo run ufa:dev:clean && find . -name 'node_modules' -type d -exec rm -rf {} + && find . -name '.moose' -type d -exec rm -rf {} + && find . -name '.ts-node' -type d -exec rm -rf {} +",
    "ufa:dev:seed": "./ufa/scripts/dev-seed.sh",
    "ufa:dev:seed:transactional-supabase-foobar": "./ufa/scripts/dev-seed.sh --service=transactional-supabase-foobar",
    "ufa:dev:seed:analytical-moose-foobar": "./ufa/scripts/dev-seed.sh --service=analytical-moose-foobar",
    "ufa:dev:seed:retrieval-elasticsearch-foobar": "./ufa/scripts/dev-seed.sh --service=retrieval-elasticsearch-foobar",
    "odw:dev": "turbo run odw:dev --ui=tui",
    "odw:dev:clean": "turbo run odw:dev:clean",
    "odw:dev:seed": "./odw/scripts/dev-seed.sh"
  },
  "devDependencies": {
    "@turbo/gen": "^1.13.4",
    "eslint": "^8.57.1",
    "prettier": "^3.6.2",
    "turbo": "^2.5.4",
    "typescript": "5.9.2"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "supabase"
    ],
    "ignoredBuiltDependencies": [
      "@swc/core",
      "esbuild"
    ]
  },
  "packageManager": "pnpm@10.14.0",
  "engines": {
    "node": ">=18"
  }
}
