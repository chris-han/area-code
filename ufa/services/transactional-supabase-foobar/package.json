{
  "name": "transactional-supabase-foobar",
  "version": "1.0.0",
  "description": "Transactional business logic API with Drizzle ORM",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "ufa:dev": "NODE_ENV=development ./scripts/dev-startup.sh",
    "ufa:dev:clean": "bun run db:stop",
    "build": "tsup src/server.ts --format esm --clean --out-dir dist",
    "ufa:prod:seed": "./database/scripts/run-sql-seed-external.sh",
    "build:copy-env": "[ -f .env.production ] && cp .env.production dist/ || echo 'Warning: .env.production file is missing. Ensure it exists if required.'",
    "dev": "./scripts/dev-startup.sh",
    "dev:fastify": "bunx tsx watch src/server.ts",
    "dev:migrate": "bun run db:migrate",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "bun run supabase migration up --workdir database",
    "db:migrate:check": "drizzle-kit check",
    "db:start": "bun run supabase start --workdir database",
    "db:stop": "bun run supabase stop --workdir database || echo 'Note: supabase stop failed (may not be running)'",
    "db:status": "bun run supabase status --workdir database",
    "health": "./scripts/health-check.sh"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "2.0.0-beta.10",
    "@ai-sdk/openai": "2.0.0-beta.12",
    "@fastify/cors": "^10.0.1",
    "@fastify/swagger": "^9.5.1",
    "@fastify/swagger-ui": "^5.2.3",
    "@supabase/realtime-js": "^2.10.2",
    "@supabase/supabase-js": "^2.39.1",
    "@workspace/models": "workspace:*",
    "ai": "5.0.0-beta.28",
    "dotenv": "^16.3.1",
    "drizzle-orm": "^0.44.4",
    "drizzle-zod": "^0.5.1",
    "fastify": "^5.4.0",
    "jwt-decode": "^4.0.0",
    "pino-pretty": "^13.0.0",
    "postgres": "^3.4.7",
    "ws": "^8.18.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@repo/eslint-config": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "@types/node": "^20.0.0",
    "@types/ws": "^8.5.10",
    "drizzle-kit": "^0.31.4",
    "supabase": "^2.31.8",
    "tsup": "^8.0.0",
    "tsx": "^4.7.0",
    "typescript": "catalog:"
  },
  "keywords": [
    "fastify",
    "drizzle",
    "orm",
    "api",
    "transactional"
  ],
  "author": "",
  "license": "MIT"
}
